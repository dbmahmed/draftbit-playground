{
  "expo": {
    "name": "Draftbit Playground",
    "slug": "draftbit-playground",
    "scheme": "draftbit-playground",
    "description": "My app powered by Draftbit",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.75.0",
    "orientation": "default",
    "icon": "./assets/app/icon.png",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5A45FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [],
    "ios": {
      "bundleIdentifier": "com.draftbit.playground",
      "supportsTablet": true,
      "buildNumber": "1.75.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to enable audio chat with others",
        "NSBluetoothAlwaysUsageDescription": "Our app requires Bluetooth to connect with energy devices, enabling efficient device management and enhanced user experience."
      },
      "usesIcloudStorage": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 75,
      "package": "com.draftbit.playground",
      "permissions": [],
      "googleServicesFile": "config/google-services.json",
      "adaptiveIcon": {}
    },
    "androidStatusBar": {},
    "androidNavigationBar": {},
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    },
    "extra": {
      "eas": {}
    }
  }
}